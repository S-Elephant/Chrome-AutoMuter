{
	"manifest_version": 3,
	"name": "Auto Tab Muter",
	"version": "1.0",
	"description": "Mutes tabs that contains specific URL matches.",
	"permissions": ["tabs", "storage"],
	"background": {
		"service_worker": "background.js"
	},
	"options_page": "options.html",
	"action": {
		"default_popup": "",
		"default_icon": {
			"16": "icon16.png",
			"48": "icon48.png",
			"128": "icon128.png"
		}
	},
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	}
}
